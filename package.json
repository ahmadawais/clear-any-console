{
	"name": "clear-any-console",
	"version": "0.10.0",
	"description": "Cross platform console clear for your next Node.js CLI.",
	"main": "index.js",
	"author": "Ahmad Awais (https://twitter.com/MrAhmadAwais/)",
	"license": "MIT",
	"keywords": [
		"clear",
		"clear any console",
		"console",
		"clear console",
		"clear console windows",
		"clear console linux",
		"clear console mac OS"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ahmadawais/clear-any-console.git"
	},
	"bugs": {
		"url": "https://github.com/ahmadawais/ClearConsole/issues"
	},
	"homepage": "https://github.com/ahmadawais/ClearConsole#readme",
	"devDependencies": {
		"auto-changelog": "^1.14.1",
		"release-it": "^12.3.4"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": ".vscode/changeLogTemplate.hbs",
		"unreleased": true,
		"commitLimit": false,
		"package": true,
		"replaceText": {
			"FIX: ": "",
			"NEW: ": "",
			"IMPROVE: ": "",
			"DOC: ": ""
		}
	},
	"scripts": {
		"release": "release-it",
		"release-major": "release-it major",
		"release-dry": "release-it --dry-run"
	}
}
